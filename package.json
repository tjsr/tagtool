{
  "name": "@tjsr/tagtool",
  "description": "A tool for storing tags against data elements",
  "version": "0.1.0",
  "main": "index.ts",
  "author": "Tim Rowe <tim@tjsr.id.au>",
  "license": "ISC",
  "homepage": "./",
  "scripts": {
    "clean": "rimraf dist build node_modules",
    "test": "ts-test",
    "generate": "prisma generate",
    "start:frontend": "cross-env GENERATE_SOURCEMAP=true PORT=3008 react-scripts start",
    "start:dev": "nodemon index.ts && cross-env PORT=3008 react-scripts start",
    "start:prod": "node index.js",
    "build": "cross-env NODE_OPTIONS=--openssl-legacy-provider react-scripts build && tsc",
    "docker:build": "docker build -t tagtool -f Dockerfile .",
    "lint": "eslint --fix \"./src/**/*.{ts,tsx}\"",
    "prettier": "prettier --single-quote --write \"./src/**/*.{ts,tsx}\"",
    "format": "npm run prettier && npm run lint",
    "db:init:dev": "npx prisma migrate dev --name init",
    "db:upgrade": "npx prisma migrate dev",
    "db:upgrade:deploy": "npx prisma migrate deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tjsr/tagtool.git"
  },
  "keywords": [
    "Tags"
  ],
  "bugs": {
    "url": "https://github.com/tjsr/tagtool/issues"
  },
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "email-validator": "^2.0.4",
    "express": "^4.18.2",
    "express-mysql-session": "^3.0.0",
    "express-session": "^1.17.3",
    "http": "^0.0.1-security",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "request-ip": "^3.3.0",
    "snowflake-uuid": "^1.0.0",
    "uuid": "^9.0.0",
    "web-vitals": "^3.3.1"
  },
  "devDependencies": {
    "@prisma/client": "^4.12.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-mysql-session": "^2.1.3",
    "@types/morgan": "^1.9.4",
    "@types/mysql": "^2.15.21",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.11",
    "@types/request-ip": "^0.0.37",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "cross-env": "^7.0.3",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prisma": "^4.12.0",
    "react-scripts": "^5.0.1",
    "rimraf": "^4.4.1",
    "typescript": "^4.9.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
